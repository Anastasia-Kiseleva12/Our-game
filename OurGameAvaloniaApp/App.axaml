<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="OurGameAvaloniaApp.App">
             

    <Application.Styles>
       <FluentTheme>
          <FluentTheme.Palettes>

             <ColorPaletteResources x:Key="Dark"/>
          </FluentTheme.Palettes>
       </FluentTheme>
           
    </Application.Styles>
	<Application.Resources>
		<FontFamily x:Key="FreckleFaceFont">avares://OurGameAvaloniaApp/Assets/Fonts#Freckle Face</FontFamily>     
	</Application.Resources>
   <Application.Styles>
      <Style Selector="Slider">
         <Setter Property="Background" Value="LightGray"/>
      </Style>
   </Application.Styles>
</Application>
